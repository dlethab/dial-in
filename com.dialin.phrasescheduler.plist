<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dialin.phrasescheduler</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>/Users/YOUR_USERNAME/Desktop/dialin/auto_phrase_scheduler.py</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/YOUR_USERNAME/Desktop/dialin</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/Users/YOUR_USERNAME/Desktop/dialin/phrase_scheduler.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/YOUR_USERNAME/Desktop/dialin/phrase_scheduler.log</string>
    
    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist> 